<UserControl x:Class="Sumerics.Controls.ScriptElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="50" Width="140">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/ScriptTheme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Canvas>
        <Border BorderThickness="1" Height="50" Width="140" CornerRadius="5"
                BorderBrush="DarkGray" Background="#FFFEFEFE">
        </Border>
    </Canvas>
</UserControl>
